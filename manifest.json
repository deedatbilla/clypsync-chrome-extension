{
    "name": "ClipSync",
    "description": "Share clipboard beetween pc and android phone",
    "version": "1.0",
    
    "manifest_version": 2,
    "background": {
      "page": "index.html",
      "persistent": false
      },
      "permissions": ["storage","https://clypsync.herokuapp.com/","notifications","background"],
      "action": {
        "default_popup": "login.html",
        "default_icon": {
            "16": "/assets/icon/48.png",
            "32": "/assets/icon/48.png",
            "48": "/assets/icon/48.png",
            "128": "/assets/icon/78.png"
          }
      },
      "icons": {
        "16": "/assets/icon/48.png",
        "32": "/assets/icon/48.png",
        "48": "/assets/icon/48.png",
        "128": "/assets/icon/78.png"
       
        
      }

    
  }