{
    "name": "ClypSync",
    "description": "Share clipboard between pc and android phone",
    "version": "1.0.3",
    "manifest_version": 2,
    "background": {
      "page": "background.html",
      "persistent": true
      },
      "permissions": ["storage","https://clypsync.herokuapp.com/","notifications","background"],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/icon/logo.png",
           
            "128": "/assets/icon/logo2.png"
          }
      },
      "icons": {
        "16": "/assets/icon/logo.png",
  
        "128": "/assets/icon/logo2.png"
      }

    
  }